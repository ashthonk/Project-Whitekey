                android:text="NoteName?" />

            <ImageView
                android:id="@+id/keyMid4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow5"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />

            <ImageView
                android:id="@+id/keyMid5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow4"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />

            <ImageView
                android:id="@+id/keyMid6"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow6"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />

