                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow9"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/fullscreen_content_controls2"
            android:layout_width="wrap_content"
            android:layout_height="250dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/TopBarLayout"
            android:orientation="horizontal"
            android:paddingBottom="10dp"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:paddingTop="0dp"
            android:weightSum="1.0" >

            <ImageView
                android:id="@+id/keyLow0"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow0"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />

            <ImageView
                android:id="@+id/keyLow1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow1"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />

            <ImageView
                android:id="@+id/keyLow2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow2"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />

            <ImageView
                android:id="@+id/keyLow3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:layout_weight="0.1"
                android:background="@drawable/keylow3"
                android:gravity="bottom|center_vertical|center_horizontal"
                android:text="NoteName?" />

            <ImageView
                android:id="@+id/keyLow4"
                android:layout_width="wrap_content"
